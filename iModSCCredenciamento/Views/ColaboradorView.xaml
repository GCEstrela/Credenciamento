        <i:FormateTel x:Key="FormateTel" />
    </UserControl.Resources>
    <Grid  Margin="5" Loaded="Frm_Loaded">

        <Grid.Resources>
            <i:ConvertBoolToImage x:Key="ConvertBoolToImage" />
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="35" />
            <RowDefinition Height="184" />
            <RowDefinition Height="30"/>
            <RowDefinition Height="241"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1000" />
        </Grid.ColumnDefinitions>

        <StackPanel Orientation="Horizontal" Margin="0,5">
            <Label Content="Pesquisar:" Height="28" Width="65" Foreground="#FFD0D0D0" Margin="1,0,0,0"/>
            <ComboBox  Width="150" IsEnabled="{Binding Comportamento.IsEnableCriar}" ItemsSource="{Binding ListaPesquisa }"
             DisplayMemberPath="Value"
             SelectedValue="Key" 
             SelectedItem="{Binding PesquisarPor}" VerticalContentAlignment="Center"/>
            <TextBox Width="200" Margin="10,0,0,0" IsEnabled="{Binding Comportamento.IsEnableCriar}" Text="{Binding NomePesquisa, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" >
                <TextBox.InputBindings>
                    <KeyBinding Command="{Binding Path=PesquisarCommand}" Key="Enter" />
                </TextBox.InputBindings>
            </TextBox>
        </StackPanel>


        <StackPanel x:Name="ListaColaboradores_sp" Grid.Column="0" Grid.Row="1" Orientation="Horizontal"  Margin="0,0,-219,0" Height="184" VerticalAlignment="Top">
                <ListView x:Name="lstView" 
                          IsEnabled="{Binding IsEnableLstView}"  
                          ItemsSource="{Binding EntityObserver, Mode=TwoWay}" 
                          SelectedItem="{Binding Entity, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"   
                          Margin="0,0,5,0" Width="1055">
                <ListView.View>
                    <GridView FrameworkElement.FlowDirection="RightToLeft">
                        <!--<GridViewColumn >
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <Image Source="{Binding Pendencias, Converter={StaticResource ConvertBoolToImage}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Width="16"  Height="16"/>
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>-->
                        <GridViewColumn Width="55" Header="Código"  DisplayMemberBinding="{Binding ColaboradorId}"/>
                        <GridViewColumn Width="115" Header="CPF"  DisplayMemberBinding="{Binding Cpf}"/>
                        <GridViewColumn Width="250"  Header="Nome do Colaborador" DisplayMemberBinding="{Binding Nome}"/>
                        <GridViewColumn Width="150"  Header="Apelido do Colaborador" DisplayMemberBinding="{Binding Apelido}"/>
                        <GridViewColumn Width="115"  Header="Telefone" DisplayMemberBinding="{Binding TelefoneFixo}"/>
                        <GridViewColumn Width="115"  Header="Celular" DisplayMemberBinding="{Binding TelefoneCelular}"/>
                        <GridViewColumn Width="200"  Header="E-Mail" DisplayMemberBinding="{Binding Email}"/>
                    </GridView>
                </ListView.View>
            </ListView>
            <StackPanel x:Name="BotoesGeral_sp" Orientation="Vertical" Width="165" Margin="0,1,0,0">

                <!--<Button x:Name="btnPesquisar" Content="Pesquisar" ToolTip="Pesquisar"  Height="30" Width="148"  Click="Pesquisar_bt_Click" Margin="0,0,0,7" />-->
                <Button x:Name="btnAdicionar" Content="Adicionar" ToolTip="Adicionar" IsEnabled="{Binding Comportamento.IsEnableCriar}" Command="{Binding PrepareCriarCommand}"   Height="31" Width="148" Margin="0,0,0,7"  />
                <Button x:Name="btnEditar" Content="Editar"  Height="31" ToolTip="Editar" IsEnabled="{Binding Comportamento.IsEnableEditar}"  Command="{Binding PrepareAlterarCommand}" Width="148"  Margin="0,0,0,7"  />
                <Button x:Name="btnExcluir" Content="Remover"  Height="31" Width="148" ToolTip="Remover" IsEnabled="{Binding Comportamento.isEnableRemover}"  Command="{Binding PrepareRemoverCommand}" Margin="0,0,0,7"  />
                <Button x:Name="btnCancelar" Content="Cancelar"  ToolTip="Cancelar"  Height="31" Width="148" IsEnabled="{Binding Comportamento.isEnableCancelar}"  Command="{Binding PrepareCancelarCommand}" Margin="0,0,0,7"  />
                <Button x:Name="btnSalvar" Content="Salvar"   ToolTip="Salvar"  Height="31" Width="148" IsEnabled="{Binding Comportamento.isEnableSalvar}"  Command="{Binding PrepareSalvarCommand}" Margin="0,0,0,7"  />

            </StackPanel>
        </StackPanel>

        <Label Content="{Binding Entity.Nome }" Grid.Column="0" Grid.Row="2" FontSize="14" Foreground="#FF007ACC" Margin="0,1,-94,2" />

        <TabControl x:Name="TabGeral_tc" 
                    SelectedIndex="{Binding SelectedTabIndex }"
                    Height="562" 
                    Width="1259" Margin="1,1,-259,-321" 
                    Grid.Row="3"    >

            <TabItem x:Name="Geral_ti"   Background="{x:Null}"    Margin="0,-2,0,0" GotFocus="Geral_ti_GotFocus">

                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Button x:Name="Geral_bt"  Style="{StaticResource MenuStyle}" Click="OnPendenciaGeral_Click" >
                            <StackPanel>
                                <Image Source="{Binding PendenciaGeral,
                                        Converter={StaticResource ConvertBoolToImage},
                                        UpdateSourceTrigger=Explicit, Mode=TwoWay}" Width="16"  Height="16" RenderTransformOrigin="1,1" />
                            </StackPanel>
                        </Button>
                        <StackPanel Width="170">
                            <TextBlock Text="Geral" HorizontalAlignment="Center"/>
                        </StackPanel>
                    </StackPanel>
                </TabItem.Header>
                <Grid Margin="5" HorizontalAlignment="Left" Width="601" IsEnabled="{Binding Comportamento.isEnableCancelar}"  >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="124*"/>
                        <ColumnDefinition Width="477*"/>
                    </Grid.ColumnDefinitions>
                    <GroupBox Style="{Styles {x:Static GroupBoxStyles.NamedSeparator}}" Header="Identificação" Background="BlueViolet"  Height="192" VerticalAlignment="Top" Margin="0,0,2,0" Grid.ColumnSpan="2">
                        <StackPanel Orientation="Horizontal" Height="172"   VerticalAlignment="Top" Margin="1">
                            <StackPanel.Resources>
                                <i:StringToImageConverter x:Key="StringToImageConverter" />
                            </StackPanel.Resources>
                            <StackPanel Margin="1,1,1,0" Height="166" VerticalAlignment="Top" Width="416">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1"   >
                                    <Label Content="Código:" Height="28" Width="100"   Foreground="#FFD0D0D0" HorizontalContentAlignment="Right" />
                                    <TextBox CharacterCasing="Upper"  x:Name="Codigo_tb"   MaxLength="50" Text="{Binding Entity.ColaboradorId, UpdateSourceTrigger=PropertyChanged}" IsEnabled="False"  Height="24"  Width="50"/>
                                    <Label Content="CPF:" Height="28" Width="132" Margin="1"  Foreground="#FFD0D0D0" HorizontalContentAlignment="Right" />
                                    <TextBox  x:Name="txtCpf"  MaxLength="20" IsEnabled="True"  Height="24"  Width="122"  Foreground="White" VerticalContentAlignment="Center" 
                                                      Text="{Binding Entity.Cpf,
                                                      UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                                      HorizontalContentAlignment="Center" LostFocus="OnValidaCpf_LostFocus" Validation.ErrorTemplate="{StaticResource validationErrorTemplate}"  />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1" >
                                    <Label Content="Nome:" Height="28" Width="100"   Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                    <TextBox CharacterCasing="Upper"  Text="{Binding Entity.Nome}"   x:Name="Nome_tb"  MaxLength="50"   IsEnabled="True"  Height="24"  Width="306"  Validation.ErrorTemplate="{StaticResource validationErrorTemplate}"/>
                                </StackPanel>

                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1"   >
                                    <Label Content="Apelido:" Height="28" Width="100"   Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                    <TextBox CharacterCasing="Upper"  x:Name="Apelido_tb"   
                                             MaxLength="50" Text="{Binding Entity.Apelido, UpdateSourceTrigger=PropertyChanged}"
                                             IsEnabled="True"  Height="24"  Width="306"/>

                                </StackPanel>

                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1" >
                                    <Label Content="Nome Pai:" Height="28"  Width="100"    Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                    <TextBox CharacterCasing="Upper"  x:Name="Ins_Estadual_tb"   MaxLength="11" PreviewTextInput="NumberOnly" Text="{Binding Entity.NomePai, UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"    Width="306"/>

                                </StackPanel>

                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1"  >
                                    <Label Content="Nome Mãe:" Height="28"  Width="100"    Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                    <TextBox CharacterCasing="Upper"  x:Name="Ins_Municipal_tb"   MaxLength="11" PreviewTextInput="NumberOnly" Text="{Binding Entity.NomeMae, StringFormat=\{0:d\}, UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="306"/>
                                </StackPanel>

                            </StackPanel>
                            <StackPanel VerticalAlignment="Top" Margin="50,6,20,0" Height="140" Width="101" >
                                <Border  CornerRadius="3"  BorderThickness="1"   Width ="100" Height="100"    HorizontalAlignment="Left" BorderBrush="#FF686868"  >
                                    <Image x:Name="Logo_im" Stretch="Uniform" Source="{Binding Entity.Foto, Converter={StaticResource StringToImageConverter}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Margin="3"   />
                                </Border>
                                <Button x:Name="SelecionarLogo_bt" Content="Selecionar Foto" HorizontalAlignment="Center" Margin="0,10,0,0"  Height="30" Width="100" Click="OnSelecionaFoto_Click"   />
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>

                    <GroupBox Style="{Styles {x:Static GroupBoxStyles.NamedSeparator}}" Background="Brown" Grid.Column="0" Header="Endereço"  Margin="0,192,0,0" Height="205" VerticalAlignment="Top" Grid.ColumnSpan="2" >
                        <StackPanel Margin="1">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1" >
                                <Label Content="Logradouro:" Height="28" Width="100"   Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                <TextBox CharacterCasing="Upper"  x:Name="Endereco_tb"   MaxLength="50" Text="{Binding Entity.Endereco, UpdateSourceTrigger=PropertyChanged}"    IsEnabled="True"  Height="24"  Width="455"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1,1,0,1" Width="562" >
                                <Label Content="No.:" Height="28" Width="100"    Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                <TextBox CharacterCasing="Upper"  x:Name="Numero_tb"   MaxLength="50" PreviewTextInput="NumberOnly" Text="{Binding Entity.Numero,  UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="50"/>
                                <Label Content="CEP:" Height="28" Width="90"    Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                <TextBox CharacterCasing="Upper"  x:Name="Cep_tb" MaxLength="50" PreviewTextInput="NumberOnly" Text="{Binding Entity.Cep,  UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="98"/>
                                <Label Content="Complemento:" Height="28" Width="89" Margin="0,0,5,0"  Foreground="#FFD0D0D0" />
                                <TextBox CharacterCasing="Upper"  x:Name="Complemento_tb"   MaxLength="50" Text="{Binding Entity.Complemento,   UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="123" RenderTransformOrigin="0.5,0.5"/>

                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left"  Width="569"   >
                                <Label Content="Bairro:" Height="28" Width="102"   Foreground="#FFD0D0D0" HorizontalContentAlignment="Right" />
                                <TextBox CharacterCasing="Upper"  x:Name="Bairro_tb"   MaxLength="50" 
                                         Text="{Binding Entity.Bairro,   UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="23"  Width="152"   />
                                <Label Content="UF:" Height="28" Width="30" Margin="1"  Foreground="#FFD0D0D0" />
                                <ComboBox x:Name="cmbEstado"   Height="25"
                                                  ItemsSource="{Binding Estados}" DisplayMemberPath="Uf" 
                                                  SelectedValue="{Binding Entity.EstadoId,  UpdateSourceTrigger=PropertyChanged}" 
                                                  SelectedValuePath="EstadoId" IsSynchronizedWithCurrentItem="True" 
                                                  SelectedIndex="0" 
                                                  SelectedItem="{Binding Estado}"
                                                  Width="53"/>

                                <Label Content="Cidade:" Height="28" Width="50"    Foreground="#FFD0D0D0"   />
                                <ComboBox x:Name="Cidade_cb" Height="25" 
                                                   ItemsSource="{Binding Municipios}" DisplayMemberPath="Nome"
                                                  SelectedValue="{Binding Entity.MunicipioId,  UpdateSourceTrigger=PropertyChanged}" 
                                                  SelectedValuePath="MunicipioId"
                                                  SelectedIndex="0"
                                                    Width="167"/>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>

                    <GroupBox Style="{Styles {x:Static GroupBoxStyles.NamedSeparator}}" Background="Crimson"    x:Name="Contato1_gb"  Header="Contatos "    VerticalAlignment="Top" Margin="486,0,-610,0" Height="192" Grid.Column="1" >
                        <StackPanel Margin="1">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1" >
                                <Label Content="Telefone:" Height="28" Width="72"    Foreground="#FFD0D0D0" HorizontalContentAlignment="Right"/>
                                <TextBox CharacterCasing="Upper"  x:Name="Contato1_tb"   MaxLength="50" Text="{Binding Entity.TelefoneFixo,Converter={StaticResource FormateTel}, UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="250"/>
                                <Label Content="Celular:" Height="28" Width="105"    Foreground="#FFD0D0D0" HorizontalContentAlignment="Right" />
                                <TextBox Foreground="#FFD0D0D0"   CharacterCasing="Upper"  x:Name="Tel1_tb"   MaxLength="15" Margin="1"
                                                     Text="{Binding Entity.TelefoneCelular, Converter={StaticResource FormateTel}, UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="98" HorizontalContentAlignment="Center"/>

                            </StackPanel>

                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left" Margin="1" >
                                <Label Content="E-Mail:" Height="28" Width="72"   Foreground="#FFD0D0D0" HorizontalContentAlignment="Right" />
                                <TextBox CharacterCasing="Lower"  x:Name="Email1_tb"   MaxLength="50" Text="{Binding Entity.Email, UpdateSourceTrigger=PropertyChanged}"  IsEnabled="True"  Height="24"  Width="454"/>

