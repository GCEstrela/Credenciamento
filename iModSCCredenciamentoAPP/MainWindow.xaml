<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iModSCCredenciamento"
        xmlns:PagePrincipal="clr-namespace:iModSCCredenciamento.PagePrincipal;assembly=iModSCCredenciamento" x:Class="iModSCCredenciamento.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Background="{x:Null}" d:DesignWidth="1000.343" d:DesignHeight="888.1" WindowState="Maximized" >

    <Grid>
        <PagePrincipal:PagePrincipalView >
            <PagePrincipal:PagePrincipalView.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.179"/>
                    <GradientStop Color="#FF2D2D30" Offset="1"/>
                </LinearGradientBrush>
            </PagePrincipal:PagePrincipalView.Background>
        </PagePrincipal:PagePrincipalView>
    </Grid>
</Window>
