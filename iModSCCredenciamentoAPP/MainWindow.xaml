<Window x:Class="IMOD.ImodApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:modulo="clr-namespace:iModSCCredenciamento;assembly=iModSCCredenciamento"
        xmlns:pagePrincipal="clr-namespace:iModSCCredenciamento.PagePrincipal;assembly=iModSCCredenciamento"
        mc:Ignorable="d" Title="Credenciamento" Background="Transparent" Width="1200" WindowState="Maximized">
    <Grid >
        <pagePrincipal:PagePrincipalView >
            <pagePrincipal:PagePrincipalView.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.179"/>
                    <GradientStop Color="#FF2D2D30" Offset="1"/>
                </LinearGradientBrush>
            </pagePrincipal:PagePrincipalView.Background>
        </pagePrincipal:PagePrincipalView>
    </Grid>
    
</Window>
